        <mxCell id="8xRxbdoGfwqq_y77huhC-0" value="&lt;p class=&quot;MsoNormal&quot;&gt;&lt;span style=&quot;color: rgb(25 , 25 , 25) ; font-size: 10.5000pt&quot;&gt;让政府改革的红利得到更多的释放&lt;/span&gt;&lt;/p&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;fontColor=#333333;" vertex="1" parent="WIyWlLk6GJQsqaUBKTNV-1">
